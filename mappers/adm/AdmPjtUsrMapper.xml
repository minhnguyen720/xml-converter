<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dou.adm.mappers.AdmPjtUsrMapper">
    <resultMap id="AdmPjtUsrMap" type="AdmPjtUsrVO">
        <result property="usrId" column="USR_ID" javaType="String"/>
        <result property="pjtId" column="PJT_ID" javaType="String"/>
        <result property="stDt" column="ST_DT" javaType="java.sql.Date"/>
        <result property="endDt" column="END_DT" javaType="java.sql.Date"/>
        <result property="roleId" column="ROLE_ID" javaType="String"/>
        <result property="createUser" column="cre_usr_id"/>
        <result property="updateUser" column="upd_usr_id"/>
        <result property="createDate" column="cre_dt"/>
        <result property="updateDate" column="upd_dt"/>
    </resultMap>
    <select id="selectByKey" parameterType="AdmPjtUsrVO" resultMap="AdmPjtUsrMap">
        select /** AdmPjtUsr.selectByKey */
            USR_ID,
            PJT_ID,
            ST_DT,
            END_DT,
            ROLE_ID,
            cre_usr_id,
            cre_dt,
            upd_usr_id,
            upd_dt
        from ADM_PJT_USR
        where 1 = 1
          and USR_ID = #{usrId}
          and PJT_ID = #{pjtId}
          and ROLE_ID = #{roleId}
    </select>

    <insert id="insert" parameterType="AdmPjtUsrVO">
        insert /** AdmPjtUsr.insert */
        into ADM_PJT_USR(USR_ID,
                         PJT_ID,
                         ST_DT,
                         END_DT,
                         ROLE_ID,
                         cre_usr_id, cre_dt, upd_usr_id, upd_dt)
        values (#{usrId},
                #{pjtId},
                #{stDt},
                #{endDt},
                #{roleId},
                #{createUser}, sysdate, #{updateUser}, sysdate)
    </insert>
</mapper>